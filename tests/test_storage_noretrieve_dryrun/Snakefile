
storage fs:
    provider="fs"


rule all:
    input:
        storage("fs/test2.txt")


checkpoint foo:
    output:
        storage("fs/test.txt")
    shell:
        "echo 'test' > test.txt"


rule bar:
    input:
        storage("fs/test.txt")
    output:
        storage("fs/test2.txt")
    shell:
        "cp {input} {output}"